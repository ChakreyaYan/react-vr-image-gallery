{
  "name": "react-vr-image-gallery",
  "version": "0.4.0",
  "scripts": {
    "start": "node -e \"console.log('open browser at http://localhost:8081/vr/\\n\\n');\" && node node_modules/react-native/local-cli/cli.js start",
    "bundle": "node node_modules/react-vr/scripts/bundle.js",
    "open": "node -e \"require('xopen')('http://localhost:8081/vr/')\"",
    "build": "babel package --ignore __tests__ --out-dir ./dist --copy-files",
    "devtools": "react-devtools",
    "test": "jest"
  },
  "main": "dist/index.js",
  "dependencies": {
    "create-react-class": "15.6.2",
    "ovrui": "~2.0.0",
    "prop-types": "15.6.0",
    "react": "16.0.0",
    "react-native": "~0.48.0",
    "react-redux": "^5.0.6",
    "react-vr": "~2.0.0",
    "react-vr-web": "~2.0.0",
    "redux": "^3.7.2",
    "three": "^0.87.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-jest": "^19.0.0",
    "babel-cli": "^6.6.4",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-plugin-transform-es2015-modules-umd": "^6.6.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "babel-preset-react-native": "^1.9.1",
    "jest": "^19.0.2",
    "react-devtools": "^2.5.2",
    "react-test-renderer": "16.0.0",
    "xopen": "1.0.0"
  },
  "jest": {
    "preset": "react-vr"
  }
}
